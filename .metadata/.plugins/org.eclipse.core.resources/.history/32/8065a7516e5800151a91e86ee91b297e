package test;
import java.util.*;


public class arrayMax_Min {
	public static void main(String args[]){
	    Scanner sc = new Scanner(System.in);
	    int a[] = new int[10];
	    for(int i=0;i<10;i++){
	    	a[i] = (int) (Math.random() * 10) + 1;
	    	
	    }
	    for(int i=0;i<10;i++){
	    System.out.print(a[i] + " ");
	    }
	    
	    int xMin = a[0];
	    
	    System.out.println();
	    
	    for(int i=1;i<10;i++){
	    	if (xMin > a[i]){
	    		xMin = a[i];
	    	}
	    }
	    System.out.println(xMin);
	}
}
