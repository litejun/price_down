
public class Iris {
	public static void main(String args[]){
		int sum = 0;
		int a = 1;
		
		//1~100까지 합, 짝수합
		for(int i=0;i<100;i++){
			sum = sum + a;
			a = a + 1;
		}
		System.out.println(sum);
		
		sum = 0;
		a = 1;
		for(int i=0;i<50;i++){
			sum = sum + a;
			a = a + 2;
		}
		System.out.println(sum);
		
		sum = 0;
		a = 2;
		for(int i=0;i<50;i++){
			sum = sum + a;
			a = a + 2;
		}
		System.out.println(sum);
		
		//정렬, 최대값구하기
		int temp;
		int max = 0;
		int array[] = {1,5,4,7,8,2,3,9,10,6};
		for(int i=0;i<array.length-1;i++){
			for(int j = 0;j<array.length-1-i;j++){
				if(array[j]>=array[j+1]){
					temp = array[j];
					array[j] = array[j+1];
					array[j+1] = temp;
				}
			}
		}
		max = array[9];
		System.out.println(max);
		for(int i = 0;i<array.length;i++){
			System.out.print(array[i] + " ");
		}
		System.out.println();
		
		//Combination 계산하기
		double combination = 0;
		double sum1 = 0, sum2 = 0, sum3 = 0;
		for(double i=1;i<=7;i++){
			sum1 = sum1 + Math.log(i);
		}

		for(int i=1;i<=3;i++){
			sum2 = sum2 + Math.log(i);
		}

		for(int i=1;i<=4;i++){
			sum3 = sum3 + Math.log(i);
		}

		
		combination = Math.exp(sum1-sum2-sum3);
		System.out.println(combination);
		
		//주차요금 계산 프로그램 실습
		int fee = 0;
		int time = 630;
		
		if(time<=10){
			fee = 0;
		}
		if(time>10 && time<=30){
			fee = 500;
		}
		if(time>30){
			fee = 500 + (time-30)/10*500;
			if(fee>30000) fee = 30000;
		}
		
		System.out.println(fee);
		
		//최소제곱법 구현
		double Price[] = {14.6, 14.1,14.0,15.6,15.6,14.7,14.5,15.7,15.1,14.8,15.2,14.7,15.6,15.6,14.6};
		double Odometer[] = {37.4, 44.8,45.8,30.9,31.7,34.0,45.9,19.1,40.1,40.2,32.4,43.5,32.7,34.5,37.7};

		double x = 0,xx = 0,y = 0,xy = 0;
		for(int i = 0;i<Price.length;i++){
			x = x + Price[i];
			y = y + Odometer[i];
			xx = xx + Price[i]*Price[i];
			xy = Price[i]*Odometer[i];
		}
		System.out.print(x + " " + y +" " + xx + " " + xy);
		System.out.println();
		double alpha, beta;
		alpha = (15*x*y-y)/(15*xx-x);
		beta = (y-alpha*x)/15;
		System.out.println("alpha = " + alpha);
		System.out.println("beta = " + beta);
		
	}
}