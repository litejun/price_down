package On2sort;
import java.util.*;

public class Bubble_sort {
	public static void main(String args[]){
		
		Scanner scan = new Scanner(System.in);
		int size;
		
		System.out.println("배열의 크기를 입력하세요");
		size = scan.nextInt();
		
		System.out.println("랜덤으로 배열에 수를 집어넣겠습니다.");
		
		System.out.println("버블 정렬을 시작합니다.");
	}
}
