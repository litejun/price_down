package test;
import java.util.*;


public class arrayMax_Min {
	public static void main(String args[]){
	    Scanner sc = new Scanner(System.in);
	    int a[] = new int[10];
	    for(int i=0;i<10;i++){
	    	a[i] = (int) (Math.random() * 10) + 1;
	    	
	    }
	    
	    System.out.println(a);
	    
	    System.out.println("배열에 넣을 숫자를 입력하세요");
	    
	    
	}
}
