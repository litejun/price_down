package test;
import java.util.*;

public class even_odd {
	public static void main(String args[]){
	    Scanner sc = new Scanner(System.in);
	    
	    System.out.println("숫자를 입력하세요");
	    int a = sc.nextInt();
	    
	    if(a/2*2==a){
	    	System.out.println("입력하신 수는 짝수입니다.");
	    }
	    else{
	    	System.out.println("입력하신 수는 홀수입니다.");
	    }
	    
	}
}
