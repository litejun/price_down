package test;
import java.util.*;


public class arrayMax_Min {
	public static void main(String args[]){
	    Scanner sc = new Scanner(System.in);
	    for(int i=0;i<10;i++){
	    	if(i != 0) System.out.println(",");
	    	System.out.print(Math.random());
	    }
	    
	    
	    System.out.println("배열에 넣을 숫자를 입력하세요");
	    
	    
	}
}
