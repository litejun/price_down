	public static void main(String args[]){
	    Scanner sc = new Scanner(System.in);
	    System.out.println("배열의 크기를 입력하세요");
	    	int a = sc.nextInt();
	    
	    
	    System.out.println("배열에 넣을 숫자를 입력하세요");
	    
	    int a[];
	    for(int i = 0;i<a.length;i++){
	    	int a[] = new int[i];
	    			sc.nextInt();
	    }
	    
	}
}
