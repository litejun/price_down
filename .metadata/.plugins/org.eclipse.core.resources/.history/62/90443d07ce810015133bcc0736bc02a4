        //스택 - 연결리스트 구현
        // 연결리스트 스택 생성
        ListStack listStack = new ListStack();
        
        System.out.println("List Stack 테스트");
        
        // 스택에 데이터 삽입
        for(int i=1; i<=5; i++){
            listStack.push("ListStack 데이터-"+i);
        }
        
        listStack.push("ListStack 데이터-6");
        
        System.out.println(listStack.pop());
        System.out.println(listStack.pop());
        System.out.println(listStack.peek());
        System.out.println(listStack.peek());    
        System.out.println(listStack.pop());
        System.out.println(listStack.pop());
        System.out.println(listStack.pop());
        System.out.println(listStack.pop());
		
    }
